{
  "luci-app-singb": {
    "description": "Grant UCI and RPC access to LuCI app singb",
    "read": {
      "file": {
        "/etc/init.d/sing-box": [ "exec" ],
        "/etc/sing-box/config.json": [ "read" ]
      },
      "uci": [ "singb" ]
    },
    "write": {
      "file": {
        "/etc/sing-box/config.json": [ "write" ]
      },
      "uci": [ "singb" ]
    }
  }
}
